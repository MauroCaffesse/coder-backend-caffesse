{{#if products.length}}
  {{#each products}}
    <ul>
      <li>Title: {{title}}</li>
      <li>Description: {{description}}</li>
      <li>Price: ${{price}}</li>
      <li>Image: {{thumbnail}}</li>
      <li>Code: {{code}}</li>
      <li>Category: {{category}}</li>
      <li>Stock: {{stock}}</li>
    </ul>
  {{/each}}
{{else}}
  <p>Product list empty</p>
{{/if}}